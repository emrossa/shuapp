<!-- views/pages/timings.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<main>
<div class="row">
    <div class="col-sm-8">    
        <div class="jumbotron">
            <h2>How long would you like for your event to run?</h2>
            <p>Choose one of our packages.</p>
            <ul>
                <% timing.forEach(function(time) { %>
                    <li><%= time.hours %> - <%= time.timeprice %></li>
                <% }); %>
            </ul> 
        </div>
    </div>

    <div class="col-sm-4">        
        <div class="well">
            <h3>My Account</h3>
        </div>
    </div>

</div>
</main>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>